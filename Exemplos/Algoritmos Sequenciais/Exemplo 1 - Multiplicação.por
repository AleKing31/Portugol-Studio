// O exemplo pede dois números reais e ao fim exibe o resultado da multiplicação dos dois números.
programa
{
	funcao inicio ()
	{
		real num1, num2, multiplicacao

		escreva("Digite dois números reais: \n")
		leia(num1, num2) // leitura dos valores de digitados

		multiplicacao = num1 * num2 // operação de multiplicação 

		escreva("A soma dos número digitado é: ", multiplicacao) // exibição do resultado
		
	}
}
