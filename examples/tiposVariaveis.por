programa {

	funcao inicio (cadeia args[]) 
	{ 
		inteiro num1
		real num2
		caracter letra
		logico vf
		cadeia nome

		escreva ("Digite um numero inteiro\n")
		leia(num1)
		escreva ("Digite um numero real\n")
		leia(num2)
		escreva ("Digite uma letra\n")
		leia(letra)
		escreva ("Digite o teu nome\n")
		leia(nome)
		escreva ("Digite um valor logico\n")
		leia(vf)

		limpa()
		
		escreva(num1,"\n")
		escreva(num2,"\n")
		escreva(letra,"\n")
		escreva(nome,"\n")
		escreva(vf,"\n")
	} 
}
